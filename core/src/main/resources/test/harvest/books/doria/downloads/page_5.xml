<div class="okvir-rob-sredina"><h3>Oglejte si tudi:</h3><script type="text/javascript"><![CDATA[function orderby(polje, vrednost)
 {
  var nv = "";
  var qs = window.location.search.substring(1);
  
  par = qs.split("&");
  
  if(par.length
< 1)
  { 
  
   for (var i=0; i
< par.length; i++) 
   { 
     var pol = par[i].indexOf("="); 
     
    if(pol >= 0) 
    { 
    var k = par[i].substring(0,pol); 
    var v = par[i].substring(pol+1); 
     
    
      nv += (i == 0 ? "" : "&amp;") + k + "=" + (k == polje ? vrednost : v);
    } 
   }  
    }
    else
    {
   nv += polje + "=" + vrednost;
    }
    
  window.location = window.location.pathname + "?" + nv;
